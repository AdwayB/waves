{
  "name": "waves",
  "version": "0.0.1",
  "main": "index.js",
  "author": "AdwayB <adwaybyju@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn && webpack serve --open --config webpack.config.js --progress --hot",
    "typescript": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky install",
    "rimraf": "rm -rf node_modules && yarn cache clean",
    "lint": "eslint . --fix --quiet --cache && prettier --write .",
    "lint:scss": "stylelint \"*/src/**/*.scss\" --quiet --cache",
    "format": "prettier --loglevel error --cache --write \"*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "runChecks": "yarn format && yarn lint && yarn typescript && yarn lint:scss",
    "versionCheck": "yarn outdated",
    "depcheck": "npx depcheck",
    "storybook": "storybook dev -p 6006"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix --quiet --cache"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md,less,html,cjs,yml,yaml,mjs}": [
      "prettier --write --cache"
    ],
    "*.{scss,css}": [
      "stylelint --quiet --cache"
    ]
  },
  "dependencies": {
    "core-js": "^3.36.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@mdx-js/loader": "^3.0.1",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@types/core-js": "^2.5.8",
    "@types/css-modules": "^1.0.5",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/http-errors": "^2.0.4",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/regenerator-runtime": "^0.13.5",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@waves/component-library": "./packages/component-library/",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "css-modules": "^0.3.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "file-loader": "^6.2.0",
    "gsap": "^3.12.5",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.8.1",
    "msw": "^2.2.2",
    "msw-storybook-addon": "^1.10.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.35",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "sass-loader": "^14.1.1",
    "source-map-loader": "^5.0.0",
    "storybook": "^7.6.17",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  }
}
