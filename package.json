{
  "name": "waves",
  "version": "0.0.1",
  "main": "index.js",
  "author": "AdwayB <adwaybyju@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn && webpack serve --open --config .webpack/webpack.dev.js --progress",
    "build": "yarn && webpack --config .webpack/webpack.prod.js --progress",
    "typescript": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky install",
    "rimraf": "rmdir /S /Q node_modules && yarn cache clean",
    "lint": "eslint . --fix --quiet --cache && prettier --write .",
    "lint:scss": "stylelint \"**/*.scss\" --quiet --cache",
    "format": "prettier --loglevel error --cache --write \"*.{js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "runChecks": "yarn format && yarn lint && yarn typescript && yarn lint:scss",
    "versionCheck": "yarn outdated",
    "depcheck": "npx depcheck",
    "storybook": "storybook dev -p 6006"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix --quiet --cache"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md,less,html,cjs,yml,yaml,mjs}": [
      "prettier --write --cache"
    ],
    "*.{scss,css}": [
      "stylelint --quiet --cache"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.12",
    "@mui/styles": "^5.15.12",
    "@mui/x-date-pickers": "^6.19.6",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "core-js": "^3.36.0",
    "dotenv": "^16.4.5",
    "material-ui-popup-state": "^5.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.2",
    "react-swipeable-views": "^0.14.0",
    "redux": "^5.0.1",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.8",
    "@types/css-modules": "^1.0.5",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-swipeable-views": "^0.13.5",
    "@types/regenerator-runtime": "^0.13.5",
